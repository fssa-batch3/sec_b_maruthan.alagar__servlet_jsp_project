<!DOCTYPE html>
<%@page import="java.util.Collections"%>
<%@page import="java.time.format.DateTimeFormatter"%>
<%@page import="in.fssa.mambilling.model.RecentBillModal"%>
<%@page import="java.util.List"%>
<html>
<%@ include file="header.jsp"%>
<head>
<link rel="stylesheet"
	href="<%=request.getContextPath()%>/assets/css/homepage/homepage.css">
<title>M A M Billing</title>
<style>
</style>
</head>

<%
List<RecentBillModal> bill_list = (List<RecentBillModal>) request.getAttribute("bills");
Collections.reverse(bill_list);

int count = 0;
for (RecentBillModal forCount : bill_list) {

	count++;
}
%>

<body>
	<div>

		<span class="arr-right"> <a
			href="<%=request.getContextPath()%>/graph.jsp">&#10095;</a></span> <a
			href="<%=request.getContextPath()%>/graph.jsp"><span
			class="arr-left">&#10094;</span></a> <br>
	</div>
	<%
	if (count == 0) {
	%>
	<div id="img">
			<a href="bills/new"><img
				src="data:image/svg+xml;base64,"
				alt="bill not found image">
			<h1>Bills yet to be Added</h1></a>
			<h2>
				<span id="span">&#x21d1;</span> Click Here to add New bill <span
					id="span">&#x21d1;</span>
			</h2>

		</div>


	<%
	} else {
	%>



	<div class="top_nav">
		<h1>Recent Bills</h1>
	</div>

	<table>
		<tr>
			<th>Bill No</th>
			<th>Customer Name</th>
			<th>Product Count</th>
			<th>Date</th>
			<th>Time</th>

		</tr>
		<%
		for (RecentBillModal bill : bill_list) {
		%>
		<tr>
			<td><%=bill.getBillId()%></td>
			<td><%=bill.getUserName()%></td>
			<td><%=bill.getProductCount()%> Products</td>

			<%
			DateTimeFormatter formatter = DateTimeFormatter.ofPattern("dd-MM-yyyy hh:mm:ss a");
			String formattedDateTime =  bill.getTime().format(formatter);
			String[] dateTimeParts = formattedDateTime.split(" ");
			String formattedDate = dateTimeParts[0];
			String formattedTime = dateTimeParts[1];
			String zone = dateTimeParts[2];
			%>

			<!-- Display date and time separately in HTML -->
			<td><%=formattedDate%></td>
			<td><%=formattedTime+" "+zone%></td>

		</tr>
		<%
		}
		%>
	</table>

	<%
	}
	%>

</body>
</html>